<?xml version="1.0" encoding="utf-8"?>
<com.open.lib.TouchView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <com.open.lib.TouchView
        android:id="@+id/shareTopBar"
        android:layout_width="fill_parent"
        android:layout_height="50dp" >

        <com.open.lib.TouchView
            android:id="@+id/groupMembersListContent "
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_gravity="left"
            android:layout_marginRight="60dp"
            android:padding="5dp" >
        </com.open.lib.TouchView>

        <com.open.lib.TouchImageView
            android:id="@+id/releaseShare"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="right"
            android:padding="10dp"
            android:src="@drawable/square_release" />
    </com.open.lib.TouchView>

</com.open.lib.TouchView>
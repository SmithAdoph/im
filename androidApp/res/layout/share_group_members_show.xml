<?xml version="1.0" encoding="utf-8"?>
<com.open.lib.TouchView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <com.open.lib.TouchView
        android:id="@+id/shareTopBar"
        android:layout_width="fill_parent"
        android:layout_height="50dp" >

        <com.open.lib.TouchView
            android:id="@+id/groupMembersListContent"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="left|center_vertical"
            android:layout_marginRight="60dp"
            android:paddingBottom="5dp"
            android:paddingTop="5dp" >
        </com.open.lib.TouchView>

        <com.open.lib.TouchImageView
            android:id="@+id/releaseShare"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_gravity="right"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:src="@drawable/square_release" />
    </com.open.lib.TouchView>

</com.open.lib.TouchView>